{
  "schema_version": 1,
  "commands": [
    {
      "id": "scripts.inventory",
      "summary": "List available scripts in rich_rust",
      "command": [
        "find",
        "scripts",
        "-maxdepth",
        "2",
        "-type",
        "f"
      ],
      "risk": "safe"
    },
    {
      "id": "scripts.sample",
      "summary": "Run a sample script in rich_rust (scripts/check_test_names.sh)",
      "command": [
        "bash",
        "scripts/check_test_names.sh"
      ],
      "risk": "ask-first"
    }
  ]
}
